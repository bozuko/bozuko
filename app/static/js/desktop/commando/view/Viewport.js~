Ext.define('Commando.view.Viewport', {
    extend: 'Ext.container.Viewport',
    title: 'Commando Container',
    requires: ['Commando.view.mongodb.Dashboard'],

    layout: 'fit',

    initComponent: function(){
        this.items = [{
            xtype: 'mongodbdashboard',
            title: 'MongoDB Stats'
        }];
        this.callParent();
    }
});